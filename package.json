{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "storybook": "cross-env TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest --run",
    "lint": "eslint --cache --fix src/*/**.{tsx,ts}",
    "format": "prettier --write src/*.{tsx,ts}",
    "isready": "npm run lint && npm run format && npm run build && npm run test:run",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets/icons --out-dir=./src/icons"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.6",
    "@tanstack/react-query": "4.13.0",
    "@tanstack/react-query-devtools": "4.13.0",
    "axios": "1.1.3",
    "date-fns": "2.29.3",
    "i18next": "22.0.3",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-facebook-login": "4.1.1",
    "react-google-login": "5.2.2",
    "react-i18next": "12.0.0",
    "react-query": "4.0.0-beta.23",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.2",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-angular": "17.1.0",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/react": "6.5.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.7",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-facebook-login": "4.1.5",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/ui": "0.24.3",
    "babel-loader": "9.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.6",
    "husky": "8.0.1",
    "jsdom": "20.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.0",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-fonts": "0.6.0",
    "vite-plugin-svgr": "2.2.2",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.24.3"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
