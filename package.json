{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest --run",
    "lint": "eslint --cache --fix src/*/**.{tsx,ts}",
    "lint:knip": "knip",
    "lint:spelling": "cspell \"**\"",
    "format": "prettier --write src/*.{tsx,ts}",
    "isready": "pnpm run lint && pnpm run lint:knip && pnpm run lint:spelling && pnpm run format && pnpm run build && pnpm run test:run",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets/icons --out-dir=./src/icons && pnpm run lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "release-it"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "@tanstack/react-query": "4.32.6",
    "@tanstack/react-query-devtools": "4.32.6",
    "axios": "1.4.0",
    "date-fns": "2.30.0",
    "i18next": "23.4.4",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "13.2.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.15.0",
    "styled-components": "6.0.7",
    "stylis": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-angular": "17.7.0",
    "@release-it/conventional-changelog": "7.0.0",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/blocks": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@svgr/cli": "8.1.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.4.9",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-v8": "0.34.2",
    "@vitest/ui": "0.34.2",
    "auto-changelog": "2.4.0",
    "cspell": "7.0.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "knip": "2.19.11",
    "lint-staged": "13.2.3",
    "prettier": "3.0.2",
    "release-it": "16.1.5",
    "storybook": "7.3.2",
    "typescript": "5.1.6",
    "unplugin-fonts": "1.0.3",
    "vite": "4.4.9",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.2"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
