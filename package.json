{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "storybook": "cross-env TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint --cache --fix src/*.{js,tsx,ts}",
    "format": "prettier --write src/*.{js,tsx,ts}",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets --out-dir=./src/icons"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "axios": "0.26.1",
    "i18next": "21.6.14",
    "qs": "6.10.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-i18next": "11.16.2",
    "react-query": "4.0.0-beta.1",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-angular": "16.2.3",
    "@storybook/addon-actions": "6.5.0-alpha.57",
    "@storybook/addon-essentials": "6.5.0-alpha.57",
    "@storybook/addon-links": "6.5.0-alpha.57",
    "@storybook/builder-vite": "0.1.24",
    "@storybook/react": "6.5.0-alpha.57",
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "14.0.4",
    "@types/node": "17.0.23",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "5.1.24",
    "@types/testing-library__jest-dom": "5.14.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@vitejs/plugin-react": "1.3.0",
    "@vitest/ui": "0.9.0",
    "babel-loader": "8.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "7.0.4",
    "jsdom": "19.0.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "vite-plugin-eslint": "1.3.0",
    "vite-plugin-fonts": "0.3.0",
    "vite-plugin-svgr": "1.1.0",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.9.0"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
