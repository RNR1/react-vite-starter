{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "storybook": "cross-env TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint --cache --fix src/*/**.{tsx,ts}",
    "format": "prettier --write src/*.{tsx,ts}",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets/icons --out-dir=./src/icons"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.3",
    "axios": "0.27.2",
    "date-fns": "2.28.0",
    "i18next": "21.8.11",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.17.4",
    "react-query": "4.0.0-beta.23",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-angular": "17.0.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-vite": "0.1.38",
    "@storybook/react": "6.5.9",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.2.1",
    "@types/node": "18.0.0",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "@vitejs/plugin-react": "1.3.2",
    "@vitest/ui": "0.16.0",
    "babel-loader": "8.2.5",
    "cross-env": "7.0.3",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.1.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.5.13",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-plugin-eslint": "1.6.1",
    "vite-plugin-fonts": "0.5.0",
    "vite-plugin-svgr": "2.2.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.16.0"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
